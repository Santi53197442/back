{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "startCommand": "npm start",
    "healthcheckPath": "/",
    "healthcheckTimeout": 10,
    "healthcheckInterval": 15,
    "healthcheckGracePeriod": 45,
    "healthcheckRetries": 3
  }
}